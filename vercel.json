{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/swagger-ui.css",
      "headers": {
        "Content-Type": "text/css"
      },
      "dest": "/api-docs/swagger-ui.css"
    },
    {
      "src": "/api-docs/swagger-ui-bundle.js",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/api-docs/swagger-ui-bundle.js"
    },
    {
      "src": "/api-docs/swagger-ui-standalone-preset.js",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/api-docs/swagger-ui-standalone-preset.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
