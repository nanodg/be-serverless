{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*\\.css)",
      "headers": {
        "Content-Type": "text/css"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
